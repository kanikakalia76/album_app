
<%= form_for @myalbum, :html => { :multipart => true } do |f| %>

  <p>
    <%= f.label :Name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :DESCRIPTION %><br>
    <%= f.text_field :description %>
  </p>

 <p>
     <%= f.file_field :photo %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>
